<!-- Accordion # Always open -->
<div class="accordion" style="" id="accordionPanelsStayOpenExample">
  <div class="accordion-item">
      <%= link_to "ALIMENTAÇÃO", "#", class: "accordion-button card-category shadow-lg", type: "button", data:{bs_toggle: "collapse", bs_target: "#panelsStayOpen-collapseOne"}, aria: {expanded: "true", controls: "panelsStayOpen-collapseOne"}, style: "background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://res.cloudinary.com/dpxsoqlbz/image/upload/v1662585740/development/image_part_001_btj89u.jpg)" %>
    <div id="panelsStayOpen-collapseOne" class="accordion-collapse collapse show" aria-labelledby="panelsStayOpen-headingOne">
      <div class="accordion-body">
        <%= render "foods/foods" %>
          <div class="card-btn-service">
            <%= button_to "+ detalhes", foods_path, method: :get, class: "btn-gradient" %>
            <%= button_to "+ adicionar", new_food_path, method: :get, class: "btn-gradient2" %>
          </div>
      </div>
    </div>
  </div>

